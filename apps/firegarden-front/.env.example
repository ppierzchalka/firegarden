# Environment Variables Example File
# Copy this file to .env.local and fill in your own values
# DO NOT commit your .env.local file to version control

# GitHub integration settings
GITHUB_USERNAME=YOUR_GITHUB_USERNAME

# Site selection for Netlify deployment
# Use "cms" to deploy the CMS app, or use front for the frontend app
SITE_APP=front

# Next.js environment variables for Firebase configuration
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
